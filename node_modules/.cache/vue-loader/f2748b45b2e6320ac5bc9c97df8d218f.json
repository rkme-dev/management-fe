ite\"\n            class=\"me-2\"\n            size=\"3.5rem\"\n            @click=\"redirect(data.link)\"\n          >\n            {{ data.icon }}\n          </v-icon>\n        </v-card-title>\n        <v-card-actions class=\"justify-space-between\">\n          <div>\n          </div>\n        </v-card-actions>\n      </v-card>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\n// eslint-disable-next-line object-curly-newline\nimport { mdiHelpCircleOutline, mdiWheelBarrow, mdiHeart, mdiShareVariant, mdiOfficeBuilding, mdiFormatListNumbered } from '@mdi/js'\nimport router from '@/router'\n\nexport default {\n  setup() {\n    const redirect = link => {\n      router.push(link)\n    }\n\n    const solidCardData = [\n      {\n        cardBg: 'info',\n        title: 'Raw Materials',\n        link: '/products',\n        cardImage: require('@/assets/images/logos/bottled-water.jpg'),\n        icon: mdiWheelBarrow,\n      },\n      {\n        cardBg: 'primary',\n        cardImage: require('@/assets/images/logos/warehouse-building.jpg'),\n        title: 'Warehouse Branches',\n        icon: mdiOfficeBuilding,\n        link: '/warehouses',\n      },\n      {\n        cardBg: 'success',\n        title: 'Inventory',\n        cardImage: require('@/assets/images/logos/waterbottles.png'),\n        icon: mdiFormatListNumbered,\n        link: '/products',\n      },\n    ]\n\n    return {\n      redirect,\n      solidCardData,\n      icons: {\n        mdiOfficeBuilding,\n        mdiHelpCircleOutline,\n        mdiWheelBarrow,\n        mdiHeart,\n        mdiShareVariant,\n      },\n    }\n  },\n}\n</script>\n"]}]}