  :right=\"$vuetify.rtl\"\n    @input=\"val => $emit('update:is-drawer-open', val)\"\n  >\n    <!-- Navigation Header -->\n    <div class=\"vertical-nav-header d-flex items-center ps-6 pe-5 pt-5 pb-2\">\n      <router-link\n        to=\"/\"\n        class=\"d-flex align-center text-decoration-none\"\n      >\n        <v-slide-x-transition>\n          <h2 class=\"app-title text--primary\">\n            Enco Group\n          </h2>\n        </v-slide-x-transition>\n      </router-link>\n    </div>\n\n    <!-- Navigation Items -->\n    <v-list\n      v-for=\"(item, index) in navigations\"\n      :key=\"index\"\n      expand\n      shaped\n      class=\"vertical-nav-menu-items pr-5\"\n    >\n      <nav-menu-link\n        v-if=\"!item.children\"\n        :title=\"item.title\"\n        :to=\"{ name: item.link }\"\n        :icon=\"item.icon\"\n      ></nav-menu-link>\n\n      <nav-menu-group\n        v-else\n        :title=\"item.title\"\n        :icon=\"item.icon\"\n      >\n        <div\n          v-for=\"(subItem, subIndex) in item.children\"\n          :key=\"subIndex\"\n        >\n          <nav-menu-link\n            :title=\"subItem.title\"\n            :to=\"{ name: subItem.link }\"\n            :icon=\"subItem.icon\"\n          ></nav-menu-link>\n        </div>\n      </nav-menu-group>\n    </v-list>\n    <!--    <v-list-->\n    <!--      expand-->\n    <!--      shaped-->\n    <!--      class=\"vertical-nav-menu-items pr-5\"-->\n    <!--    >-->\n    <!--      <nav-menu-link-->\n    <!--        title=\"Dashboard\"-->\n    <!--        :to=\"{ name: 'dashboard' }\"-->\n    <!--        :icon=\"icons.mdiHomeOutline\"-->\n    <!--      ></nav-menu-link>-->\n    <!--      &lt;!&ndash;      <nav-menu-link&ndash;&gt;-->\n    <!--      &lt;!&ndash;        title=\"Account Settings\"&ndash;&gt;-->\n    <!--      &lt;!&ndash;        :to=\"{ name: 'pages-account-settings'}\"&ndash;&gt;-->\n    <!--      &lt;!&ndash;        :icon=\"icons.mdiAccountCogOutline\"&ndash;&gt;-->\n  