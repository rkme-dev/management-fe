ss=\"align-center d-flex flex-column justify-center align-center\">\n          <span class=\"white--text text-center align-center\">{{ data.title }}</span>\n          <v-icon\n            color=\"white\"\n            size=\"3.5rem\"\n          >\n            {{ data.icon }}\n          </v-icon>\n        </v-card-title>\n        <v-card-actions class=\"justify-space-between\">\n          <div></div>\n        </v-card-actions>\n      </v-card>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\n// eslint-disable-next-line object-curly-newline\nimport { mdiOfficeBuilding, mdiFormatListNumbered, mdiAccountBoxMultipleOutline, mdiAccountKey } from '@mdi/js'\nimport router from '@/router'\n\nexport default {\n  setup() {\n    const redirect = link => {\n      router.push(link)\n    }\n\n    const accessControlMenu = [\n      {\n        cardBg: 'info',\n        title: 'Users',\n        link: '/users',\n        cardImage: require('@/assets/images/pages/card-basic-user.jpeg'),\n        icon: mdiAccountBoxMultipleOutline,\n      },\n      {\n        cardBg: 'primary',\n        cardImage: require('@/assets/images/pages/card-basic-department.webp'),\n        title: 'Department',\n        icon: mdiOfficeBuilding,\n        link: '/departments',\n      },\n      {\n        cardBg: 'success',\n        title: 'Access Levels',\n        cardImage: require('@/assets/images/pages/card-basic-permission.png'),\n        icon: mdiAccountKey,\n        link: '/access-levels',\n      },\n      {\n        cardBg: 'warning',\n        title: 'Roles',\n        cardImage: require('@/assets/images/pages/card-basic-permission.png'),\n        icon: mdiAccountKey,\n        link: '/roles',\n      },\n      {\n        cardBg: 'error',\n        title: 'Abilities',\n        cardImage: require('@/assets/images/pages/card-basic-permission.png'),\n        icon: mdiAccountKey,\n        link: '/abilities',\n      },\n    ]\n\n    return {\n      redirect,\n      accessControlMenu,\n      icons: {\n        mdiOfficeBuilding,\n        mdiAccountBoxMultipleOutline,\n        mdiFormatListNumbered,\n        mdiAccountKey,\n      },\n    }\n  },\n}\n</script>\n<style lang=\"scss\"></style>\n"]}]}