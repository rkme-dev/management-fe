\n      >\n        <v-img\n          class=\"auth-tree\"\n          height=\"150\"\n          :src=\"data.cardImage\"\n        ></v-img>\n        <v-card-title class=\"align-center d-flex flex-column justify-center align-center\">\n          <span class=\"white--text text-center align-center\">{{ data.title }}</span>\n          <v-icon\n            color=\"white\"\n            size=\"3.5rem\"\n          >\n            {{ data.icon }}\n          </v-icon>\n        </v-card-title>\n        <v-card-actions class=\"justify-space-between\">\n          <div></div>\n        </v-card-actions>\n      </v-card>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\n// eslint-disable-next-line object-curly-newline\nimport {\n  mdiCash,\n  mdiFolderStarMultipleOutline,\n  mdiHomeGroup,\n  mdiApplicationSettings,\n  mdiAccountCog,\n  mdiOfficeBuilding,\n  mdiPackageVariant,\n  mdiFormatListNumbered,\n  mdiAccountBoxMultipleOutline,\n  mdiHeadPlusOutline,\n  mdiInboxFullOutline,\n  mdiAccountKey\n} from '@mdi/js'\nimport router from '@/router'\n\nexport default {\n  setup() {\n    const redirect = link => {\n      router.push(link)\n    }\n\n    const accessControlMenu = [\n      {\n        cardBg: 'info',\n        title: 'Accounts',\n        link: '/accounts',\n        cardImage: require('@/assets/images/pages/master-file/accounts.jpg'),\n        icon: mdiAccountCog,\n      },\n      {\n        cardBg: 'info',\n        title: 'Documents',\n        link: '/documents',\n        cardImage: require('@/assets/images/pages/master-file/documents.jpg'),\n        icon: mdiApplicationSettings,\n      },\n      {\n        cardBg: 'info',\n        title: 'Locations',\n        link: '/locations',\n        cardImage: require('@/assets/images/pages/master-file/locations.jpg'),\n        icon: mdiHomeGroup,\n      },\n      {\n        cardBg: 'info',\n        title: 'Salesman',\n        link: '/salesman',\n        cardImage: require('@/assets/images/pages/master-file/salesperson.jpg'),\n        ico