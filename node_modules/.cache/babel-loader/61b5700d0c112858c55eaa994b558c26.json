A;IAeAC,UAfA,sBAeAD,IAfA,EAeA;MACA;MACA;MACA;IACA,CAnBA;IAqBAE,iBArBA,+BAqBA;MACA;MACA;IACA,CAxBA;IA0BAC,KA1BA,mBA0BA;MAAA;;MACA;MACA;QACA;QACA;MACA,CAHA;IAIA,CAhCA;IAkCAC,WAlCA,yBAkCA;MAAA;;MACA;MACA;QACA;QACA;MACA,CAHA;IAIA,CAxCA;IA0CAC,IA1CA,kBA0CA;MACA;QACA;QACAC;MACA,CAHA,MAGA;QACA;MACA;;MACA;IACA;EAlDA;AAzDA","names":["data","search","dialog","dialogDelete","headers","text","align","sortable","value","editedIndex","editedItem","name","contact_person","email","tin","contact_no","defaultItem","mdiAccountEdit","mdiDeleteCircle","computed","formTitle","mapGetters","watch","val","mounted","methods","initialize","editItem","item","deleteItem","deleteItemConfirm","close","closeDelete","save","Object"],"sourceRoot":"src/views/pages/sales/clients","sources":["ClientsListTable.vue"],"sourcesContent":["<template>\n  <v-data-table\n    :headers=\"headers\"\n    :items=\"customers\"\n    sort-by=\"name\"\n    class=\"elevation-1\"\n    :search=\"search\"\n  >\n    <template v-slot:top>\n      <v-toolbar\n        flat\n      >\n        <v-toolbar-title>Clients</v-toolbar-title>\n        <v-divider\n          class=\"mx-4\"\n          inset\n          vertical\n        ></v-divider>\n        <v-spacer></v-spacer>\n        <v-text-field\n          v-model=\"search\"\n          append-icon=\"mdi-magnify\"\n          label=\"Search\"\n          single-line\n          hide-details\n          class=\"mr-4\"\n        ></v-text-field>\n        <v-dialog\n          v-model=\"dialog\"\n          max-width=\"500px\"\n        >\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn\n              color=\"primary\"\n              dark\n              class=\"mb-2\"\n              v-bind=\"attrs\"\n              v-on=\"on\"\n            >\n              Add New Client\n            </v-btn>\n          </template>\n          <v-card>\n            <v-card-title>\n              <span class=\"text-h5\">{{ formTitle }}</span>\n            </v-card-title>\n\n            <v-card-text>\n              <v-container>\n                <v-row>\n                  <v-col\n                    cols=\"12\"\n                    sm=\"6\"\n                    md=\"4\"\n                  >\n                    <v-text-field\n                      v-model=\"editedItem.name\"\n                      label=\"Name\"\n                    ></v-text-field>\n                  </v-col>\n                  <v-col\n                    cols=\"12\"\n                    sm=\"6\"\n                    md=\"4\"\n                  >\n                    <v-text-field\n                      v-model=\"editedItem.contact_person\"\n                      label=\"Contact Person\"\n                    ></v-text-field>\n                  </v-col>\n                  <v-col\n                    cols=\"12\"\n                    sm=\"6\"\n                    md=\"4\"\n                  >\n                    <v-text-field\n                      v-model=\"editedItem.email\"\n                      label=\"Email\"\n                    ></v-text-field>\n                  </v-col>\n                  <v-col\n                    cols=\"12\"\n                    sm=\"6\"\n                    md=\"4\"\n                  >\n                    <v-text-field\n                      v-model=\"editedItem.tin\"\n                      label=\"Tin\"\n                    ></v-text-field>\n                  </v-col>\n                  <v-col\n                    cols=\"12\"\n                    sm=\"6\"\n                    md=\"4\"\n                  >\n                    <v-text-field\n                      v-model=\"editedItem.contact_no\"\n                      label=\"Contact Number\"\n                    ></v-text-field>\n                  </v-col>\n                </v-row>\n              </v-container>\n            </v-card-text>\n\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn\n                color=\"blue darken-1\"\n                text\n                @click=\"close\"\n              >\n                Cancel\n              </v-btn>\n              <v-btn\n                color=\"blue darken-1\"\n                text\n                @click=\"save\"\n              >\n                Save\n              </v-btn>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n        <v-dialog\n          v-model=\"dialogDelete\"\n          max-width=\"500px\"\n        >\n          <v-card>\n            <v-card-title class=\"text-h5\">\n              Are you sure you want to delete this item?\n            </v-card-title>\n            <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn\n                color=\"blue darken-1\"\n                text\n                @click=\"closeDelete\"\n              >\n                Cancel\n              </v-btn>\n              <v-btn\n                color=\"blue darken-1\"\n                text\n                @click=\"deleteItemConfirm\"\n              >\n                OK\n              </v-btn>\n              <v-spacer></v-spacer>\n            </v-card-actions>\n          </v-card>\n        </v-dialog>\n      </v-toolbar>\n    </template>\n    <template v-slot:item.actions=\"{ item }\">\n      <v-icon\n        small\n        class=\"mr-2\"\n        @click=\"editItem(item)\"\n      >\n        {{ mdiAccountEdit }}\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(item)\"\n      >\n        {{ mdiDeleteCircle }}\n      </v-icon>\n    </template>\n    <template v-slot:no-data>\n      <v-btn\n        color=\"primary\"\n        @click=\"initialize\"\n      >\n        Reset\n      </v-btn>\n    </template>\n  </v-data-table>\n</template>\n\n<script>\nimport {mdiAccountEdit, mdiDeleteCircle} from \"@mdi/js\";\nimport {GET_CUSTOMERS, UPDATE_CUSTOMER} from \"@/store/actions.type\";\nimport {mapGetters} from \"vuex\";\n\nexport default {\n  data: () => ({\n    search: '',\n    dialog: false,\n    dialogDelete: false,\n    headers: [\n      {\n        text: 'Name',\n        align: 'start',\n        sortable: false,\n        value: 'name',\n      },\n      {text: 'Contact Person', value: 'contact_person'},\n      {text: 'Email', value: 'email'},\n      {text: 'Tin', value: 'tin'},\n      {text: 'Contact Number', value: 'contact_no'},\n      {text: 'Actions', value: 'actions', sortable: false},\n    ],\n    editedIndex: -1,\n    editedItem: {\n      name: '',\n      contact_person: '',\n      email: '',\n      tin: '',\n      contact_no: '',\n    },\n    defaultItem: {\n      name: '',\n      contact_person: '',\n      email: '',\n      tin: '',\n      contact_no: '',\n    },\n    mdiAccountEdit,\n    mdiDeleteCircle\n  }),\n\n  computed: {\n    formTitle() {\n      return this.editedIndex === -1 ? 'New Item' : 'Edit Item'\n    },\n    ...mapGetters([\"customers\", \"customer\"])\n  },\n\n  watch: {\n    dialog(val) {\n      val || this.close()\n    },\n    dialogDelete(val) {\n      val || this.closeDelete()\n    },\n  },\n\n  mounted() {\n    this.initialize()\n  },\n\n  methods: {\n    initialize() {\n      this.$store.dispatch(GET_CUSTOMERS)\n    },\n\n    editItem(item) {\n      this.editedIndex = this.customers.indexOf(item)\n\n      this.customer.editedIndex = item.id\n      this.customer.editedItem = item\n\n      this.editedItem = {...item}\n      this.dialog = true\n    },\n\n    deleteItem(item) {\n      this.editedIndex = this.customers.indexOf(item)\n      this.editedItem = {...item}\n      this.dialogDelete = true\n    },\n\n    deleteItemConfirm() {\n      this.customers.splice(this.editedIndex, 1)\n      this.closeDelete()\n    },\n\n    close() {\n      this.dialog = false\n      this.$nextTick(() => {\n        this.editedItem = {...this.defaultItem}\n        this.editedIndex = -1\n      })\n    },\n\n    closeDelete() {\n      this.dialogDelete = false\n      this.$nextTick(() => {\n        this.editedItem = {...this.defaultItem}\n        this.editedIndex = -1\n      })\n    },\n\n    save() {\n      if (this.editedIndex > -1) {\n        this.$store.dispatch(UPDATE_CUSTOMER, this.editedItem)\n        Object.assign(this.customers  [this.editedIndex], this.editedItem)\n      } else {\n        this.customers.push(this.editedItem)\n      }\n      this.close()\n    },\n  },\n}\n</script>\n"]}]}