arehouses) {\n      state.warehouses = warehouses\n      state.loading = false\n    },\n    fetchWarehouseSuccess(state, warehouse) {\n      state.warehouse = warehouse\n      state.loading = false\n    },\n    setErrors(state, errors) {\n      state.errors = errors\n    },\n    setLoading(state, loading) {\n      state.loading = loading\n    },\n  },\n}\n"],"mappings":";;AAAA,OAAOA,gBAAP,MAA6B,4BAA7B;AAEA,IAAMC,gBAAgB,GAAG,IAAID,gBAAJ,EAAzB,C,CACA;;AACA,OAAO,IAAME,cAAc,GAAG;EAC5BC,UAAU,EAAE,IADgB;EAE5BC,KAAK,EAAE;IACLC,OAAO,EAAE,KADJ;IAELC,MAAM,EAAE,EAFH;IAGLC,UAAU,EAAE,EAHP;IAILC,SAAS,EAAE;EAJN,CAFqB;EAS5BC,OAAO,EAAE;IACP;IACAC,MAFO,wBAEYF,SAFZ,EAEuB;MAAA,IAArBG,MAAqB,QAArBA,MAAqB;MAC5BA,MAAM,CAAC,YAAD,EAAe,IAAf,CAAN;MAEA,OAAOV,gBAAgB,CAACS,MAAjB,CAAwBF,SAAxB,EACJI,IADI,EAEH;MACA,UAAAJ,SAAS,EAAI;QACX,IAAIA,SAAS,CAACK,IAAV,CAAeC,EAAnB,EAAuB;UACrBH,MAAM,CAAC,cAAD,EAAiBH,SAAS,CAACK,IAA3B,CAAN;UACAF,MAAM,CAAC,WAAD,EAAc,EAAd,CAAN;QACD,CAHD,MAGO;UACLA,MAAM,CAAC,WAAD,EAAcH,SAAS,CAACK,IAAxB,CAAN;QACD;;QAED,OAAOE,OAAO,CAACC,OAAR,CAAgBR,SAAhB,CAAP;MACD,CAZE,EAaH,UAAAS,KAAK;QAAA,OAAIF,OAAO,CAACG,MAAR,CAAeD,KAAf,CAAJ;MAAA,CAbF,EAeJE,OAfI,CAeI,YAAM;QACbR,MAAM,CAAC,YAAD,EAAe,KAAf,CAAN;MACD,CAjBI,CAAP;IAkBD,CAvBM;IAwBPS,YAxBO,+BAwBkB;MAAA,IAAVT,MAAU,SAAVA,MAAU;MACvBA,MAAM,CAAC,WAAD,EAAc,EAAd,CAAN;IACD,CA1BM;IA2BPU,eA3BO,kCA2BqBb,SA3BrB,EA2BgC;MAAA,IAArBG,MAAqB,SAArBA,MAAqB;MACrCA,MAAM,CAAC,YAAD,EAAe,IAAf,CAAN;MAEA,OAAOH,SAAS,CAACc,MAAV,CAAiBd,SAAjB,EACJI,IADI,GAEJO,OAFI,CAEI,YAAM;QACbR,MAAM,CAAC,YAAD,EAAe,KAAf,CAAN;MACD,CAJI,CAAP;IAKD,CAnCM;IAoCPY,aApCO,gCAoCmB;MAAA,IAAVZ,MAAU,SAAVA,MAAU;MACxBA,MAAM,CAAC,YAAD,EAAe,IAAf,CAAN;MAEA,OAAOV,gBAAgB,CAACsB,aAAjB,GAAiCX,IAAjC,CACL,UAAAL,UAAU,EAAI;QACZI,MAAM,CAAC,wBAAD,EAA2BJ,UAAU,CAACM,IAAtC,CAAN;QACAF,MAAM,CAAC,YAAD,EAAe,KAAf,CAAN;QAEA,OAAOI,OAAO,CAACC,OAAR,CAAgBT,UAAhB,CAAP;MACD,CANI,EAOL,UAAAU,KAAK;QAAA,OAAIF,OAAO,CAACG,MAAR,CAAeD,KAAf,CAAJ;MAAA,CAPA,CAAP;IASD,CAhDM;IAiDPO,YAjDO,+BAiDkBV,EAjDlB,EAiDsB;MAAA,IAAdH,MAAc,SAAdA,MAAc;MAC3BA