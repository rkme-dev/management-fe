n          size=\"20\"\n          class=\"me-3\"\n        >\n          {{ tab.icon }}\n        </v-icon>\n        <span>{{ tab.title }}</span>\n      </v-tab>\n    </v-tabs>\n\n    <!-- tabs item -->\n    <v-tabs-items v-model=\"tab\">\n      <v-tab-item>\n        <client-info :information-data=\"clientInfo\"></client-info>\n      </v-tab-item>\n\n      <v-tab-item>\n        <sale-history :sales-data=\"null\"></sale-history>\n      </v-tab-item>\n    </v-tabs-items>\n  </v-card>\n</template>\n\n<script>\nimport { mdiAccountOutline, mdiCartPlus } from '@mdi/js'\nimport { ref, computed, reactive } from '@vue/composition-api'\n\n// demos\nimport SaleHistory from './SaleHistory.vue'\nimport ClientInfo from './ClientInfo.vue'\nimport {GET_CUSTOMER} from \"@/store/actions.type\";\nimport store from '@/store'\nimport router from '@/router'\nimport {CustomersService} from \"@/utils/api.service\";\nimport {onMounted} from \"@vue/composition-api/dist/vue-composition-api\";\n\nexport default {\n  components: {\n    SaleHistory,\n    ClientInfo,\n  },\n  setup() {\n    const id = router.history.current.params.id\n    store.dispatch(GET_CUSTOMER, router.history.current.params.id)\n\n    const tab = ref('')\n    let clientInfo = ref({})\n\n    onMounted(async () => {\n      await CustomersService.find(id)\n        .then(response => {\n          console.log(response)\n          clientInfo.value = response.data.data\n        })\n    })\n\n    // tabs\n    const tabs = [\n      {title: 'Client Information', icon: mdiAccountOutline},\n      {title: 'Sales history', icon: mdiCartPlus},\n    ]\n\n\n    return {\n      tab,\n      tabs,\n      clientInfo,\n      icons: {\n        mdiAccountOutline,\n        mdiCartPlus,\n      },\n    }\n  },\n}\n</script>\n"]}]}