"sourceRoot":"src/layouts/components/vertical-nav-menu","sources":["VerticalNavMenu.vue"],"sourcesContent":["<template>\n  <v-navigation-drawer\n    :value=\"isDrawerOpen\"\n    app\n    floating\n    width=\"260\"\n    class=\"app-navigation-menu\"\n    :right=\"$vuetify.rtl\"\n    @input=\"val => $emit('update:is-drawer-open', val)\"\n  >\n    <!-- Navigation Header -->\n    <div class=\"vertical-nav-header d-flex items-center ps-6 pe-5 pt-5 pb-2\">\n      <router-link\n        to=\"/\"\n        class=\"d-flex align-center text-decoration-none\"\n      >\n<!--        <v-img-->\n<!--          :src=\"require('@/assets/images/logos/logo.svg')\"-->\n<!--          max-height=\"30px\"-->\n<!--          max-width=\"30px\"-->\n<!--          alt=\"logo\"-->\n<!--          contain-->\n<!--          eager-->\n<!--          class=\"app-logo me-3\"-->\n<!--        ></v-img>-->\n        <v-slide-x-transition>\n          <h2 class=\"app-title text--primary\">\n            Enco Group\n          </h2>\n        </v-slide-x-transition>\n      </router-link>\n    </div>\n\n    <!-- Navigation Items -->\n    <v-list\n      expand\n      shaped\n      class=\"vertical-nav-menu-items pr-5\"\n    >\n      <nav-menu-link\n        title=\"Dashboard\"\n        :to=\"{ name: 'dashboard' }\"\n        :icon=\"icons.mdiHomeOutline\"\n      ></nav-menu-link>\n      <nav-menu-link\n        title=\"Account Settings\"\n        :to=\"{ name: 'pages-account-settings'}\"\n        :icon=\"icons.mdiAccountCogOutline\"\n      ></nav-menu-link>\n      <nav-menu-group\n        title=\"Pages\"\n        :icon=\"icons.mdiFileOutline\"\n      >\n        <nav-menu-link\n          title=\"Login\"\n          :to=\"{ name: 'pages-login' }\"\n          target=\"_blank\"\n        ></nav-menu-link>\n        <nav-menu-link\n          title=\"Register\"\n          :to=\"{ name: 'pages-register' }\"\n          target=\"_blank\"\n        ></nav-menu-link>\n        <nav-menu-link\n          title=\"Error\"\n          :to=