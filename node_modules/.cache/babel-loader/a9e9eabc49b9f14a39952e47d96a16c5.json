ws/pages/user-access-management","sources":["UserAccessMenu.vue"],"sourcesContent":["<template>\n  <v-row>\n    <v-col cols=\"12\">\n      <div class=\"ms-3\">\n        <p class=\"text-2xl mt-14 mb-1\">User Control</p>\n      </div>\n    </v-col>\n\n    <v-col v-for=\"data in accessControlMenu\" :key=\"data.icon\" md=\"4\" lg=\"4\" sm=\"6\" xs=\"12\">\n      <v-card :elevation=\"24\" :color=\"data.cardBg\" dark class=\"text-center\" @click=\"redirect(data.link)\">\n        <v-img class=\"auth-tree\" height=\"150\" :src=\"data.cardImage\"></v-img>\n        <v-card-title class=\"align-center d-flex flex-column justify-center align-center\">\n          <span class=\"white--text text-center align-center\">{{ data.title }}</span>\n          <v-icon color=\"white\" size=\"3.5rem\">\n            {{ data.icon }}\n          </v-icon>\n        </v-card-title>\n        <v-card-actions class=\"justify-space-between\">\n          <div></div>\n        </v-card-actions>\n      </v-card>\n    </v-col>\n  </v-row>\n</template>\n\n<script>\n// eslint-disable-next-line object-curly-newline\nimport { mdiOfficeBuilding, mdiFormatListNumbered, mdiAccountBoxMultipleOutline, mdiAccountKey } from '@mdi/js'\nimport router from '@/router'\n\nexport default {\n  setup() {\n    const redirect = link => {\n      router.push(link)\n    }\n\n    const accessControlMenu = [\n      {\n        cardBg: 'info',\n        title: 'Users',\n        link: '/users',\n        cardImage: require('@/assets/images/pages/card-basic-user.jpeg'),\n        icon: mdiAccountBoxMultipleOutline,\n      },\n      {\n        cardBg: 'primary',\n        cardImage: require('@/assets/images/pages/card-basic-department.webp'),\n        title: 'Department',\n        icon: mdiOfficeBuilding,\n        link: '/departments',\n      },\n      {\n        cardBg: 'success',\n        title: 'Access Levels',\n        cardImage: require('@/assets/images/pages/card-basic-permission.png'),\n        icon: mdiAccountKey,\n        link: '/access-levels',\n      },\n      {\n        cardBg: 'warning',\n        title: 'Roles',\n        cardImage: require('@/assets/images/pages/card-basic-permission.png'),\n        icon: mdiAccountKey,\n        link: '/roles',\n      },\n      {\n        cardBg: 'error',\n        title: 'Abilities',\n        cardImage: require('@/assets/images/pages/card-basic-permission.png'),\n        icon: mdiAccountKey,\n        link: '/abilities',\n      },\n    ]\n\n    return {\n      redirect,\n      accessControlMenu,\n      icons: {\n        mdiOfficeBuilding,\n        mdiAccountBoxMultipleOutline,\n        mdiFormatListNumbered,\n        mdiAccountKey,\n      },\n    }\n  },\n}\n</script>\n<style lang=\"scss\"></style>\n"]}]}