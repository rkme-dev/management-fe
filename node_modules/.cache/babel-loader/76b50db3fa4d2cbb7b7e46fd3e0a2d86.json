 result = []\n      if (statuses.length === 0) {\n        state.salesmans = state.originalData\n      } else {\n        state.originalData.forEach(row => {\n          if (statuses.includes(row.is_active)) {\n            result.push(row)\n          }\n        })\n\n        state.salesmans = result\n      }\n    },\n    addSalesman(state, salesman) {\n      state.salesmans.push(salesman)\n    },\n    updateSalesmanSuccess(state, salesman) {\n      const index = state.salesmans.findIndex(value => value.id === salesman.id)\n      state.salesmans[index] = salesman\n    },\n    fetchSalesmansSuccess(state, salesmans) {\n      state.salesmans = salesmans.map(rowData => {\n        // eslint-disable-next-line no-param-reassign\n        rowData.is_active = rowData.is_active === true || rowData.is_active === 'Active' ? 'Active' : 'Inactive'\n\n        return rowData\n      })\n\n      state.originalData = salesmans.map(rowData => {\n        // eslint-disable-next-line no-param-reassign\n        rowData.is_active = rowData.is_active === true || rowData.is_active === 'Active' ? 'Active' : 'Inactive'\n\n        return rowData\n      })\n\n      state.loading = false\n    },\n    fetchSalesmanSuccess(state, salesman) {\n      state.salesman = salesman\n      state.loading = false\n    },\n    setErrors(state, errors) {\n      state.errors = errors\n    },\n    setLoading(state, loading) {\n      state.loading = loading\n    },\n  },\n}\n"],"mappings":";;;;;;AAAA,OAAOA,eAAP,MAA4B,2BAA5B;AAEA,IAAMC,eAAe,GAAG,IAAID,eAAJ,EAAxB,C,CACA;;AACA,OAAO,IAAME,aAAa,GAAG;EAC3BC,UAAU,EAAE,IADe;EAE3BC,KAAK,EAAE;IACLC,OAAO,EAAE,KADJ;IAELC,MAAM,EAAE,EAFH;IAGLC,SAAS,EAAE,EAHN;IAILC,YAAY,EAAE,EAJT;IAKLC,QAAQ,EAAE;EALL,CAFoB;EAU3BC,OAAO,EAAE;IACP;IACAC,MAFO,wBAEYF,QAFZ,EAEsB;MAAA,IAApBG,MAAoB,QAApBA,MAAoB;MAC3BA,MAAM,CAAC,YAAD,EAAe,IAAf,CAAN;MAEA,OAAOX,eAAe,CAACU,MAAhB,CAAuBF,QAAvB,EACJI,IADI,EAEH;MACA,UAAAJ,QAAQ,EAAI;QACV,IAAIA,QAAQ,CAACK,IAAT,CAAcC,EAAlB,EAAsB;UACpBH,MAAM,CAAC,aAAD,EAAgBH,QAAQ,CAACK,