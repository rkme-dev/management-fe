vBA;MA6BAC;QACAhC,QADA;QAEA6B,UAFA;QAGAC,SAHA;QAIAC;MAJA,CA7BA;MAmCAE;IAnCA;EAAA,CAzCA;EA+EAC;IACAC,SADA,uBACA;MACA;MACA;MAEA;IACA,CANA;IAOAC,IAPA,kBAOA;MACA;IACA,CATA;IAUAC,OAVA,qBAUA;MACA;IACA;EAZA,CA/EA;EA8FAtC;IACAqB,MADA,kBACAkB,GADA,EACA;MACA;IACA,CAHA;IAIAjB,YAJA,wBAIAiB,GAJA,EAIA;MACA;IACA;EANA,CA9FA;EAsGAC,OAtGA,qBAsGA;IACA;IACA;EACA,CAzGA;EA0GAC;IACAC,kBADA,gCACA;MACA;MACA;IACA,CAJA;IAKAC,eALA,2BAKAhB,KALA,EAKA;MAAA;;MACA;MACA;MACA;MACA;MAEAiB;QAAA;MAAA;IACA,CAZA;IAaAC,QAbA,oBAaAC,IAbA,EAaA;MACA;MACA;MACA;MAEA;IACA,CAnBA;IAqBAC,UArBA,sBAqBAD,IArBA,EAqBA;MACA;MACA;MACA;IACA,CAzBA;IA2BAE,iBA3BA,+BA2BA;MACA;MACAvC,8EACA;QACA;UACAA;QACA;MACA,CALA;MAOA;IACA,CArCA;IAuCAwC,KAvCA,mBAuCA;MAAA;;MACA;MACA;QACA;QACA;MACA,CAHA;IAIA,CA7CA;IA+CAC,WA/CA,yBA+CA;MAAA;;MACA;MACA;QACA;QACA;MACA,CAHA;IAIA,CArDA;IAuDAC,IAvDA,kBAuDA;MACA;QACAC;MACA,CAFA,MAEA;QACA;MACA;;MACA;IACA;EA9DA;AA1GA","names":["mdiAccountEdit","mdiCancel","ref","watch","name","components","FinishProductFormWithForm","setup","Active","Inactive","title","id","store","alert","statusFilter","statusOptions","icons","search","statusColor","data","editDialog","message","type","show","dialog","dialogDelete","headers","text","align","sortable","value","editedIndex","editedItem","active","units","actions","defaultItem","mode","computed","formTitle","list","loading","val","created","methods","addOrUpdateClicked","closeUserCreate","setTimeout","editItem","item","deleteItem","deleteItemConfirm","close","closeDelete","save","Object"],"sourceRoot":"src/views/pages/master-files/finish-products","sources":["FinishProductListTable.vue"],"sourcesContent":["<template>\n  <div>\n    <v-alert\n      :type=\"alert.type\"\n      dismissible\n      :value=\"alert.show\"\n    >\n      {{ alert.message }}\n    </v-alert>\n    <v-data-table\n      :headers=\"headers\"\n      :items=\"list\"\n      :search=\"search\"\n      class=\"elevation-1 font-weight-light\"\n      :loading=\"loading\"\n      sort-by=\"name\"\n      fixed-header\n    >\n      <!-- active -->\n      <template #[`item.active`]=\"{item}\">\n        <div class=\"d-flex justify-center\">\n          <v-chip\n            small\n            :class=\"`${statusColor[item.active]}--text`\"\n            class=\"v-chip-light-bg text-center\"\n          >\n            {{ item.active }}\n          </v-chip>\n        </div>\n      </template>\n      <template v-slot:top>\n        <v-toolbar\n          flat\n        >\n          <v-col\n            cols=\"12\"\n            offset-md=\"12\"\n            md=\"2\"\n          >\n            <v-toolbar-title>Products</v-toolbar-title>\n          </v-col>\n          <v-divider\n            class=\"mx-2\"\n            inset\n            vertical\n          ></v-divider>\n          <v-card-text>\n            <v-row>\n              <v-col\n                cols=\"4\"\n              >\n                <v-text-field\n                  v-model=\"search\"\n                  :append-icon=\"icons.mdiMagnify\"\n                  label=\"Search\"\n                  single-line\n                  hide-details\n                ></v-text-field>\n              </v-col>\n              <v-col\n                cols=\"4\"\n                offset-md=\"4\"\n              >\n                <v-select\n                  v-model=\"statusFilter\"\n                  dense\n                  outlined\n                  multiple\n                  :prepend-inner-icon=\"icons.mdiHumanEdit\"\n                  item-text=\"title\"\n                  item-value=\"id\"\n                  label=\"Status\"\n                  :items=\"statusOptions\"\n                  hide-details=\"auto\"\n                ></v-select>\n              </v-col>\n            </v-row>\n          </v-card-text>\n          <v-dialog\n            v-model=\"editDialog\"\n            fullscreen\n            class=\"pa-16\"\n          >\n            <finish-product-form-with-form\n              :mode=\"mode\"\n              :row=\"editedItem\"\n              @submit=\"close\"\n            >\n            </finish-product-form-with-form>\n          </v-dialog>\n          <v-btn\n            color=\"primary\"\n            dark\n            class=\"mb-2\"\n            @click.prevent=\"editItem({})\"\n          >\n            New Product\n          </v-btn>\n          <v-dialog\n            v-model=\"dialogDelete\"\n            max-width=\"500px\"\n          >\n            <v-card>\n              <v-card-title>\n                Are you sure you want to deactivate this? ?\n              </v-card-title>\n              <v-card-actions>\n                <v-spacer></v-spacer>\n                <v-btn\n                  color=\"error\"\n                  text\n                  @click=\"closeDelete\"\n                >\n                  Cancel\n                </v-btn>\n                <v-btn\n                  color=\"success\"\n                  text\n                  @click=\"deleteItemConfirm\"\n                >\n                  OK\n                </v-btn>\n                <v-spacer></v-spacer>\n              </v-card-actions>\n            </v-card>\n          </v-dialog>\n        </v-toolbar>\n      </template>\n      <template v-slot:item.actions=\"{ item }\">\n        <v-tooltip\n          bottom\n          color=\"primary\"\n        >\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-icon\n              medium\n              class=\"mr-2\"\n              v-bind=\"attrs\"\n              v-on=\"on\"\n              @click.prevent=\"editItem(item)\"\n            >\n              {{ icons.mdiAccountEdit }}\n            </v-icon>\n          </template>\n          <span>Edit</span>\n        </v-tooltip>\n\n        <v-tooltip\n          v-if=\"item.active === 'Active'\"\n          bottom\n          color=\"primary\"\n        >\n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-icon\n              v-if=\"item.active === 'Active'\"\n              small\n              v-bind=\"attrs\"\n              v-on=\"on\"\n              @click=\"deleteItem(item)\"\n            >\n              {{ icons.mdiCancel }}\n            </v-icon>\n          </template>\n          <span>Deactivate</span>\n        </v-tooltip>\n      </template>\n      <template v-slot:no-data>\n        <v-btn\n          color=\"primary\"\n        >\n          Refresh\n        </v-btn>\n      </template>\n    </v-data-table>\n  </div>\n</template>\n\n<script>\nimport {\n  mdiAccountEdit,\n  mdiCancel,\n} from '@mdi/js'\nimport {\n  ref,\n  watch,\n} from '@vue/composition-api'\nimport FinishProductFormWithForm from '@/views/pages/master-files/finish-products/FinishProductWithUnitForm.vue'\nimport store from '@/store'\n\nexport default {\n  name: 'FinishProductListTable',\n  components: {\n    FinishProductFormWithForm,\n  },\n  setup() {\n    const statusColor = {\n      Active: 'success',\n      Inactive: 'error',\n    }\n\n    const statusFilter = ref('')\n    const statusOptions = ref([\n      {\n        title: 'Active',\n        id: 'Active',\n      },\n      {\n        title: 'Inactive',\n        id: 'Inactive',\n      },\n    ])\n\n    const search = ref()\n\n    watch(statusFilter, () => {\n      store.dispatch('FinishProductStore/filter', statusFilter.value)\n    })\n\n    return {\n      alert,\n      statusFilter,\n      statusOptions,\n      icons: {\n        mdiCancel,\n        mdiAccountEdit,\n      },\n      search,\n      statusColor,\n    }\n  },\n  data: () => ({\n    editDialog: false,\n    alert: {\n      message: '',\n      type: 'success',\n      show: false,\n    },\n    dialog: false,\n    dialogDelete: false,\n    headers: [\n      {\n        text: 'ID',\n        align: 'start',\n        sortable: true,\n        value: 'id',\n      },\n      { text: 'Name', value: 'name' },\n      {\n        text: 'Status', value: 'active', align: 'center', sortable: true,\n      },\n      { text: 'Actions', value: 'actions', sortable: false },\n    ],\n    editedIndex: -1,\n    editedItem: {\n      active: '',\n      name: '',\n      units: [],\n      actions: '',\n    },\n    defaultItem: {\n      name: '',\n      active: '',\n      units: [],\n      actions: '',\n    },\n    mode: 'create',\n  }),\n\n  computed: {\n    formTitle() {\n      // eslint-disable-next-line vue/no-side-effects-in-computed-properties\n      this.mode = this.editedIndex === -1 ? 'create' : 'edit'\n\n      return this.editedIndex === -1 ? 'New Product' : 'Edit Product'\n    },\n    list() {\n      return this.$store.state.FinishProductStore.list\n    },\n    loading() {\n      return this.$store.state.FinishProductStore.loading\n    },\n  },\n\n  watch: {\n    dialog(val) {\n      return val || this.close()\n    },\n    dialogDelete(val) {\n      return val || this.closeDelete()\n    },\n  },\n  created() {\n    this.$store.dispatch('FinishProductStore/list')\n    this.$store.dispatch('FinishProductStore/removeErrors')\n  },\n  methods: {\n    addOrUpdateClicked() {\n      this.dialog = false\n      this.editedItem = this.defaultItem\n    },\n    closeUserCreate(value) {\n      this.dialog = false\n      this.alert.message = value.alertMessage\n      this.alert.type = value.alertType\n      this.alert.show = true\n\n      setTimeout(() => { this.alert.show = false }, 1500)\n    },\n    editItem(item) {\n      this.editedIndex = this.list.indexOf(item)\n      this.editedItem = this.list[this.editedIndex]\n      this.editDialog = true\n\n      this.mode = item.id === undefined ? 'create' : 'edit'\n    },\n\n    deleteItem(item) {\n      this.editedIndex = this.list.indexOf(item)\n      this.editedItem = { ...item }\n      this.dialogDelete = true\n    },\n\n    deleteItemConfirm() {\n      this.list[this.editedIndex].active = 0\n      store.dispatch('FinishProductStore/update', this.list[this.editedIndex]).then(\n        response => {\n          if (response.active === undefined) {\n            store.dispatch('FinishProductStore/list')\n          }\n        },\n      )\n      this.closeDelete()\n    },\n\n    close() {\n      this.editDialog = false\n      this.$nextTick(() => {\n        this.editedItem = { ...this.defaultItem }\n        this.editedIndex = -1\n      })\n    },\n\n    closeDelete() {\n      this.dialogDelete = false\n      this.$nextTick(() => {\n        this.editedItem = { ...this.defaultItem }\n        this.editedIndex = -1\n      })\n    },\n\n    save() {\n      if (this.editedIndex > -1) {\n        Object.assign(this.users[this.editedIndex], this.editedItem)\n      } else {\n        this.users.push(this.editedItem)\n      }\n      this.close()\n    },\n  },\n}\n</script>\n"]}]}