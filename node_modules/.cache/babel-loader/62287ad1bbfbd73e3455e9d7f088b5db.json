e(ability)\n          },\n          error => Promise.reject(error),\n        )\n    },\n    updateRole({ commit }, role) {\n      commit('setLoading', true)\n\n      return userAccessManagementService.updateRole(role)\n        .then(\n          // eslint-disable-next-line no-shadow\n          role => {\n            commit('updateRoleSuccess', role.data)\n            commit('setLoading', false)\n\n            return Promise.resolve(role)\n          },\n          error => Promise.reject(error),\n        )\n    },\n  },\n  mutations: {\n    // eslint-disable-next-line no-shadow\n    addAbilitySuccess(state, ability) {\n      state.abilities.push(ability)\n    },\n    updateAbilitySuccess(state, ability) {\n      const index = state.abilities.findIndex(value => value.id === ability.id)\n      state.abilities[index] = ability\n    },\n    updateRoleSuccess(state, role) {\n      const index = state.roles.findIndex(value => value.id === role.id)\n      state.roles[index] = role\n    },\n    fetchAbilitiesSuccess(state, abilities) {\n      state.loading = false\n      state.abilities = abilities\n    },\n    fetchRolesSuccess(state, roles) {\n      state.loading = false\n      state.roles = roles\n    },\n    fetchUsersSuccess(state, users) {\n      state.users = users.map(user => {\n        // eslint-disable-next-line no-param-reassign\n        user.status = user.status.charAt(0).toUpperCase() + user.status.slice(1)\n\n        return user\n      })\n      state.loading = false\n    },\n    setLoading(state, loading) {\n      state.loading = loading\n    },\n  },\n}\n"],"mappings":";;;;AAAA,OAAOA,2BAAP,MAAwC,uCAAxC;AAEA,IAAMC,2BAA2B,GAAG,IAAID,2BAAJ,EAApC,C,CACA;;AACA,OAAO,IAAME,yBAAyB,GAAG;EACvCC,UAAU,EAAE,IAD2B;EAEvCC,KAAK,EAAE;IACLC,OAAO,EAAE,KADJ;IAELC,SAAS,EAAE,EAFN;IAGLC,KAAK,EAAE,EAHF;IAILC,OAAO,EAAE,EAJJ;IAKLC,KAAK,EAAE;EALF,CAFgC;EASvCC,OAAO,EAAE;IACP;IACAC,aAFO,+BAEmBC,OAFnB,EAE4B;MAAA,IAAnBC,MAAmB,QAAnBA,MAAmB;MACjCA,MAAM,CAAC,YAAD,EAAe,IAAf,CAAN;MAEA,OAAOZ,2BAA2